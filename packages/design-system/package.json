{
  "name": "@portfolio/ui",
  "description": "A collection of react components.",
  "version": "0.0.2",
  "author": "Hamza Bahlaouane (https://void.fr/)",
  "repository": {
    "directory": "packages/design-system",
    "type": "git",
    "url": "https://bitbucket.org/adminvoid/rma-sinitres/"
  },
  "sideEffects": false,
  "main": "lib/index.js",
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "license": "MIT",
  "directories": {
    "lib": "lib",
    "test": "__test__"
  },
  "files": [
    "lib",
    "types",
    "src"
  ],
  "scripts": {
    "dev": "tsc --watch --preserveWatchOutput",
    "build": "tsc --build tsconfig.json",
    "postbuild": "yarn icons:build && cp ./public/icons.svg ./lib/icons.svg",
    "clean": "rm -rf node_modules && rm -rf lib && rm -rf types",
    "lint": "TIMING=1 eslint src/",
    "lint:fix": "eslint --fix src/",
    "lint:reports": "eslint src -f ../../node_modules/eslint-detailed-reporter/lib/detailed.js -o eslint-report/index.html",
    "test": "jest",
    "test:coverage": "jest --coverage --maxWorkers=1 --coverageDirectory='coverage'",
    "types:check": "tsc --noEmit",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build",
    "storybook-docs": "storybook dev --docs",
    "build-storybook-docs": "storybook build --docs",
    "icons:build": "svg-spreact ./icons --optimize true --tidy true > ./public/icons.svg"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@portfolio/tailwind-config": "*",
    "@tanstack/react-table": "^8.9.1",
    "@visx/visx": "^3.6.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "clsx": "^2.0.0",
    "cogo-toast": "^4.2.3",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.30.0",
    "keen-slider": "^6.8.6",
    "react-day-picker": "^8.7.1",
    "react-simple-star-rating": "^5.1.7",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@headlessui/react": "^1.7.14",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/blocks": "^7.5.3",
    "@storybook/builder-webpack5": "^7.5.3",
    "@storybook/nextjs": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-table": "^8.9.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.10",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-detailed-reporter": "^0.9.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-check-file": "^2.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-custom": "*",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.23",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3",
    "svgson": "^5.2.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.2"
  }
