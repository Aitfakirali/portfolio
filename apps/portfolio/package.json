{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "wireit",
    "watch:dev": "next dev",
    "build": "wireit",
    "start": "next start",
    "analyze": "ANALYZE=true yarn build",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "lint:reports": "eslint src -f ../../node_modules/eslint-detailed-reporter/lib/detailed.js -o eslint-report/index.html",
    "typecheck": "tsc --noEmit --incremental false",
    "ts:check": "tsc --project tsconfig.json --declaration && echo 'done'",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:coverage": "jest --coverage --maxWorkers=1 --coverageDirectory='coverage'",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-docs": "storybook dev --docs",
    "build-storybook-docs": "storybook build --docs",
    "sync:icons": "cp ../../node_modules/@portfolio/ui/lib/icons.svg ./public/icons.svg",
    "postinstall": "patch-package"
  },
  "wireit": {
    "dev": {
      "command": "next dev",
      "dependencies": [
        "../../packages/tailwind-config:build",
        "../../packages/http-client:build",
        "../../packages/core:build"
      ]
    },
    "build": {
      "command": "rm -rf build && next build",
      "dependencies": [
        "../../packages/tailwind-config:build",
        "../../packages/http-client:build",
        "../../packages/core:build"
      ]
    }
  },
  "overrides": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@portfolio/core": "^0.0.1",
    "@portfolio/http-client": "^0.0.1",
    "@portfolio/tailwind-config": "^0.0.1",
    "@portfolio/ui": "^0.0.2",
    "@tanstack/react-table": "^8.9.1",
    "check-password-strength": "^2.0.7",
    "clsx": "^2.0.0",
    "cogo-toast": "^4.2.3",
    "date-fns": "^2.30.0",
    "keen-slider": "^6.8.5",
    "lodash": "^4.17.21",
    "lodash.deburr": "^4.1.0",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.11.0",
    "react-day-picker": "^8.7.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-query": "^3.39.3",
    "react-simple-star-rating": "^5.1.7",
    "react-use": "^17.4.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@next/bundle-analyzer": "^14.0.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/nextjs": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.10",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "copy-to-clipboard": "^3.3.1",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-detailed-reporter": "^0.9.0",
    "eslint-plugin-check-file": "^2.3.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-custom": "^0.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "msw": "^2.0.8",
    "next-router-mock": "^0.9.10",
    "postcss": "^8.4.23",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "raw-loader": "^4.0.2",
    "storybook": "^7.5.3",
    "svg-spreact-cli": "^1.2.0",
    "svgson": "^5.2.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier -w",
      "yarn workspace portfolio eslint src",
      "yarn workspace portfolio ts:check"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  }
}
